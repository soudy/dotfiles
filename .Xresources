! urxvt*font: xft:berry:size=8
! urxvt*boldFont: xft:berry:size=8
urxvt*font: xft:Monospace:bold:size=8
urxvt*boldFont: xft:Monospace:bold:size=8
urxvt.letterSpace:-1
Xft.dpi:96
Xft.antialias:1
Xft.hinting:0
Xft.hintstyle:hintnone
Xft.autohint: false
Xft.rgba:rgb
Xft.lcdfilter:lcddefault

! clickable urls
urxvt*matcher.pattern.1:  \\b(mailto|http|https|ftp|file):[/]*[\\w-]\\.[\\w./?&@#-]*[\\w/-]
urxvt*matcher.pattern.2: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
urxvt.perl-ext-common: default,matcher
urxvt.url-launcher:    /usr/bin/firefox
urxvt.matcher.button:  1
